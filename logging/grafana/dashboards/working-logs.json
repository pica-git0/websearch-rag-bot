{
  "id": null,
  "title": "Working Logs Dashboard",
  "tags": ["logs"],
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "Total Logs",
      "type": "stat",
      "datasource": "Elasticsearch",
      "targets": [
        {
          "query": "*",
          "metrics": [
            {
              "id": "1",
              "type": "count"
            }
          ],
          "timeField": "@timestamp"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
    },
    {
      "id": 2,
      "title": "Logs by Service",
      "type": "piechart",
      "datasource": "Elasticsearch", 
      "targets": [
        {
          "query": "*",
          "metrics": [
            {
              "id": "1",
              "type": "count"
            }
          ],
          "bucketAggs": [
            {
              "id": "2",
              "type": "terms",
              "field": "service.keyword",
              "settings": {
                "size": "5"
              }
            }
          ],
          "timeField": "@timestamp"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
    },
    {
      "id": 3,
      "title": "Request Count Over Time",
      "type": "timeseries",
      "datasource": "Elasticsearch",
      "targets": [
        {
          "query": "type:request",
          "metrics": [
            {
              "id": "1",
              "type": "count"
            }
          ],
          "bucketAggs": [
            {
              "id": "2",
              "type": "date_histogram",
              "field": "@timestamp",
              "settings": {
                "interval": "5m"
              }
            }
          ],
          "timeField": "@timestamp"
        }
      ],
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8}
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "30s"
}