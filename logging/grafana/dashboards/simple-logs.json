{
  "title": "Simple Logs Test",
  "tags": ["test"],
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "Total Log Count",
      "type": "stat",
      "datasource": "Elasticsearch",
      "targets": [
        {
          "query": "*",
          "metrics": [
            {
              "id": "1",
              "type": "count"
            }
          ],
          "timeField": "@timestamp"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
    },
    {
      "id": 2,
      "title": "Logs Over Time",
      "type": "timeseries",
      "datasource": "Elasticsearch",
      "targets": [
        {
          "query": "*",
          "metrics": [
            {
              "id": "1",
              "type": "count"
            }
          ],
          "bucketAggs": [
            {
              "id": "2",
              "type": "date_histogram",
              "field": "@timestamp",
              "settings": {
                "interval": "auto"
              }
            }
          ],
          "timeField": "@timestamp"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "30s"
}