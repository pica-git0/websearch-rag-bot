{
  "title": "WebSearch RAG Bot - Logs Analysis",
  "tags": ["rag-bot", "logs"],
  "style": "dark",
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "Log Volume by Service",
      "type": "stat",
      "datasource": "Elasticsearch",
      "targets": [
        {
          "query": "*",
          "metrics": [
            {
              "id": "1",
              "type": "count"
            }
          ],
          "bucketAggs": [
            {
              "id": "2",
              "type": "terms",
              "field": "service.keyword",
              "settings": {
                "size": "10",
                "order": "desc",
                "orderBy": "_count"
              }
            }
          ],
          "timeField": "@timestamp"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
    },
    {
      "id": 2,
      "title": "Error Rate",
      "type": "timeseries",
      "datasource": "Elasticsearch",
      "targets": [
        {
          "query": "type:error",
          "metrics": [
            {
              "id": "1",
              "type": "count"
            }
          ],
          "bucketAggs": [
            {
              "id": "2",
              "type": "date_histogram",
              "field": "@timestamp",
              "settings": {
                "interval": "1m"
              }
            }
          ],
          "timeField": "@timestamp"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
    },
    {
      "id": 3,
      "title": "Performance Metrics",
      "type": "timeseries",
      "datasource": "Elasticsearch",
      "targets": [
        {
          "query": "duration:*",
          "metrics": [
            {
              "id": "1",
              "type": "avg",
              "field": "duration"
            }
          ],
          "bucketAggs": [
            {
              "id": "2",
              "type": "date_histogram",
              "field": "@timestamp",
              "settings": {
                "interval": "1m"
              }
            }
          ],
          "timeField": "@timestamp"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
    },
    {
      "id": 4,
      "title": "Application Events Over Time", 
      "type": "timeseries",
      "datasource": "Elasticsearch",
      "targets": [
        {
          "query": "type:application",
          "metrics": [
            {
              "id": "1",
              "type": "count"
            }
          ],
          "bucketAggs": [
            {
              "id": "2",
              "type": "date_histogram",
              "field": "@timestamp",
              "settings": {
                "interval": "5m"
              }
            }
          ],
          "timeField": "@timestamp"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
    },
    {
      "id": 5,
      "title": "HTTP Request Rate",
      "type": "timeseries",
      "datasource": "Elasticsearch",
      "targets": [
        {
          "query": "type:request",
          "metrics": [
            {
              "id": "1",
              "type": "count"
            }
          ],
          "bucketAggs": [
            {
              "id": "2",
              "type": "date_histogram",
              "field": "@timestamp",
              "settings": {
                "interval": "1m"
              }
            }
          ],
          "timeField": "@timestamp"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
    },
    {
      "id": 6,
      "title": "Performance Logs",
      "type": "stat",
      "datasource": "Elasticsearch",
      "targets": [
        {
          "query": "type:performance",
          "metrics": [
            {
              "id": "1",
              "type": "count"
            }
          ],
          "timeField": "@timestamp"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "30s"
}
